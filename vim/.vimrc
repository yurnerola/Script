colorscheme molokai
set t_Co=256
set autoindent
syntax on
set tabstop=4
set nu
filetype plugin indent on
set completeopt=longest,menu
set shiftwidth=4
let Tlist_Show_One_File=1
let Tlist_Exit_OnlyWindow=1
let g:winManagerWindowLayout='FileExplorer|TagList'
nmap wm :WMToggle<cr>
set cscopequickfix=s-,c-,d-,i-,t-,e-
